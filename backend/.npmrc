# Use public npm registry for all packages (avoids 403 when global config is strict)
registry=https://registry.npmjs.org/

# Optional: keep JSR for @jsr scope if you use it
@jsr:registry=https://npm.jsr.io/

# Reduce 403/rate-limit issues: fewer parallel downloads, more retries with backoff
network-concurrency=2
fetch-retries=5
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
